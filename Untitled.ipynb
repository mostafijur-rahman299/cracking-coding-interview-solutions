    "consq = 0\n",
    "\n",
    "for index in range(len(string)):\n",
    "    consq += 1\n",
    "    if index + 1 == len(string) or string[index] != string[index+1]:\n",
    "        new_string += f\"{string[index]}{consq}\"\n",
    "        consq = 0\n",
    "        \n",
    "print(new_string)"
